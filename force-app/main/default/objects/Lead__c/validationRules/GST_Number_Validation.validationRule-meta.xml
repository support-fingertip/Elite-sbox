<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>GST_Number_Validation</fullName>
    <active>true</active>
    <errorConditionFormula>NOT(ISBLANK(  GST_Number__c )) &amp;&amp; LEN( GST_Number__c ) &lt;&gt; 15</errorConditionFormula>
    <errorDisplayField>GST_Number__c</errorDisplayField>
    <errorMessage>Please Enter 15 digit correct GST number</errorMessage>
</ValidationRule>
