<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Aadhaar_No_validation</fullName>
    <active>false</active>
    <errorConditionFormula>NOT(AND(LEN(   Aadhar_Number__c ) = 12,ISNUMBER( Aadhar_Number__c))) &amp;&amp; NOT(ISBLANK( Aadhar_Number__c))</errorConditionFormula>
    <errorDisplayField>Aadhar_Number__c</errorDisplayField>
    <errorMessage>Please Enter Valid Aadhar Number</errorMessage>
</ValidationRule>
